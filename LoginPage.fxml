<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="LoginPageAnchorPane" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="model_thana.LoginPageController">
   <children>
      <Label layoutX="93.0" layoutY="74.0" text="Username">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="95.0" layoutY="113.0" text="Password">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="196.0" layoutY="77.0" />
      <PasswordField fx:id="password" layoutX="196.0" layoutY="116.0" />
      <Button fx:id="loginButton" layoutX="186.0" layoutY="202.0" mnemonicParsing="false" onMouseClicked="#loginButtonOnClick" text="Login">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Button>
      <Button fx:id="createAnAccountButton" layoutX="137.0" layoutY="253.0" mnemonicParsing="false" onMouseClicked="#createAnAccountButtonOnClick" text="Create an account">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Button>
      <Label layoutX="96.0" layoutY="150.0" text="User Type">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBox" layoutX="195.0" layoutY="153.0" prefWidth="150.0" />
   </children>
</AnchorPane>
